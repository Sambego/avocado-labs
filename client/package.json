{
    "name": "avocado-labs-client",
    "description": "Avocado Labs Landing page",
    "version": "1.0.0",
    "author": "Sam Bellen <sam.bellen@auth0.com>",
    "bugs": {
        "url": "https://github.com/contentful-userland/gatsby-contentful-starter/issues"
    },
    "dependencies": {
        "babel-plugin-styled-components": "^1.10.7",
        "gatsby-image": "^2.3.4",
        "gatsby-plugin-client-side-redirect": "^1.1.0",
        "gatsby-plugin-google-tagmanager": "^2.3.4",
        "gatsby-plugin-next-seo": "^1.5.1",
        "gatsby-plugin-react-helmet": "^3.2.4",
        "gatsby-plugin-react-svg": "^3.0.0",
        "gatsby-plugin-sharp": "^2.5.6",
        "gatsby-plugin-sitemap": "^2.4.4",
        "gatsby-plugin-styled-components": "^3.3.2",
        "gatsby-source-contentful": "^2.2.9",
        "gatsby-source-youtube": "github:Sambego/gatsby-source-youtube",
        "gatsby-transformer-remark": "^2.7.3",
        "gatsby-transformer-sharp": "^2.4.6",
        "lodash": "^4.17.15",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-helmet": "^6.0.0",
        "react-helmet-async": "^1.0.6",
        "react-markdown": "^4.3.1",
        "react-waypoint": "^9.0.2",
        "sharp": "^0.25.2",
        "styled-components": "^5.1.1"
    },
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "chalk": "^4.0.0",
        "contentful-import": "^7.7.8",
        "dotenv": "^8.2.0",
        "eslint": "^6.8.0",
        "eslint-plugin-react": "^7.19.0",
        "gatsby": "^2.20.25",
        "gh-pages": "^2.2.0",
        "inquirer": "^7.1.0",
        "prettier": "^2.0.4",
        "rimraf": "^3.0.2",
        "yargs-parser": "^18.1.3"
    },
    "homepage": "https://github.com/contentful-userland/gatsby-contentful-starter#readme",
    "keywords": [
        "gatsby",
        "contentful"
    ],
    "license": "MIT",
    "main": "n/a",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/contentful-userland/gatsby-contentful-starter.git"
    },
    "scripts": {
        "dev": "npm run develop",
        "develop": "gatsby develop",
        "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
        "test": "echo \"Error: no test specified\" && exit 1",
        "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md' 'bin/*.js'",
        "build": "gatsby build",
        "deploy": "gatsby build --prefix-paths && gh-pages -d public",
        "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
        "postinstall": "node ./bin/hello.js",
        "setup": "node ./bin/setup.js",
        "start": "npm run develop",
        "heroku-postbuild": "gatsby build",
        "cleanup-repository": "yarn remove contentful-import chalk inquirer && node ./bin/cleanup.js && rimraf bin contentful"
    },
    "engines": {
        "node": "12.16.x"
    }
}
